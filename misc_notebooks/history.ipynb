def get_batch(train_data,train_labels,batch_size = 32):
    start = 0
    end = batch_size
    while(start<=train_data.shape[0]):
        yield(torch.from_numpy(train_data[start:end]).float().to(device),torch.from_numpy(train_labels[start:end]).long().to(device))
        start+=batch_size
        end+=batch_size
