        self.relu3 = nn.ReLU()
        
        self.maxpool3 = nn.MaxPool2d(kernel_size=3)
        self.Dropout(p=0.3)
        
        self.fc1 = nn.Linear(256*25*25, 192)
        self.Dropout(p=0.3)
        
        self.fc2 = nn.Linear(192, 10)
        self.softmax = torch.nn.Softmax(10)
        
