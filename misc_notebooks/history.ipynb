    param.requires_grad = False

# advbox demo
m = PytorchModel(
    model, None,(-3, 3),
    channel_axis=1)
attack = FGSMT(m)

# 静态epsilons
attack_config = {"epsilons": 0.001, "epsilon_steps": 1, "steps": 100}

