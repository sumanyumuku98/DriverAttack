# Neural Network

class Net(nn.Module):
    
    def __init__(self, ):
        super(Net, self).__init__()
        self.fc1 = nn.Linear(10, 50)
        self.fc1.weight.data.normal_(0, 0.1)
        self.out = nn.Linear(50, N_ACTIONS)
        self.out.weight.data.normal_(0, 0.1)
        
    def forward(self, x):
        x = self.fc1(x)
        x = F.relu(x)
        actions_value = self.out(x)
        return actions_value
