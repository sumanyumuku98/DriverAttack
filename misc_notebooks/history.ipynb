def return_policy_evaluation_linalg(p, r, T, gamma):
    u = np.zeros(225)
    for s in range(225):
        if not np.isnan(p[s]):
            action = int(p[s])
            u[s] = np.linalg.solve(np.identity(225) - gamma*T[:,:,action], r)[s]
    return u
