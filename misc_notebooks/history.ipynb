        out = self.relu1(out)
        
        out = self.maxpool1(out)
        
        out = self.conv2(out)
        out = self.relu2(out)
        
        out = self.maxpool2(out)
        
        out = self.conv3(out)
        out = self.relu3(out)
        
        out = self.maxpool3(out)
        
        out = out.view(out.size(0), -1)
        
        out = self.fc1(out)
        out = self.fc2(out)
        out = self.softmax(out)
        
        return out
