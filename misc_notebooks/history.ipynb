def get_reward(dqn_prev_state, dqn_curr_state, rnn_state):
    R_human = r_human(convert2str(rnn_state), convert2str(dqn_curr_state))
    R_energy = r_energy(convert2str(dqn_curr_state))
    reward = R_human + R_energy
    return reward
