class ToTensor(object):
    
    def __call__(self, sample):
        image, landmarks = sample['image'], sample['landmarks']
        
        image = image.transpose((2, 0, 1))
        
        return {'image': torch.from_numpy(image), 'landmarks': torch.from_numpy(landmarks)}
